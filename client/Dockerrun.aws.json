{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "abufathima/multi-nginx",
      "hostname": "client",
      "essential": false
    },
    {
      "name": "server",
      "image": "abufathima/multi-server",
      "hostname": "api",
      "essential": false
    },
    {
      "name": "woker",
      "image": "abufathima/multi-worker",
      "hostname": "worker",
      "essential": false
    },
    {
      "name": "nginx",
      "image": "abufathima/nginx",
      "hostname": "nginx",
      "essential": false,
      "portMappings": {
        "hostPort": 80,
        "containerPort": 80
      },
      "links":["client","server"]
    }
  ]
}
